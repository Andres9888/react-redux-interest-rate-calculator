{
    "env": {
        "browser": true,
        "es6": true
    },
    "parser": "babel-eslint",
    "extends": [
        "react-app",
        "airbnb",
        "plugin:jsx-a11y/recommended",
        "prettier"
    ],
    "plugins": [
        "jsx-a11y",
        "prettier"
    ],
    "rules": {
        "import/prefer-default-export": "off",
        "react/react-in-jsx-scope": "off",
        "import/no-unresolved": "off",
        "import/extensions": "off",
        "react/jsx-no-bind": "off",
        "no-unused-vars": "warn",
        "semi": 0,
        "react/jsx-filename-extension": [
            1,
            {
                "extensions": [
                    ".js",
                    ".jsx"
                ]
            }
        ],
        "prettier/prettier": [
            "error",
            {
                "semi": true
            }
        ]
    }
}